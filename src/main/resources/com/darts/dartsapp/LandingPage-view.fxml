<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.darts.dartsapp.controller.LandingPageController"
      alignment="CENTER"
      spacing="20"
      prefHeight="400.0" prefWidth="600.0">

<!--    Page heading-->
    <Label text="DartsApp" style="-fx-font-size: 40px; -fx-font-weight: bold;"/>

<!--     Logo-->
    <ImageView fx:id="imageView" fitWidth="150" fitHeight="150"  preserveRatio="true"/>

<!--      Sign Up button-->
    <Button fx:id="SignUp"
            text="Get Started"
            style="-fx-font-size: 16px; -fx-padding: 10px 20px;"
            onAction="#onButtonSignUpClick"/>

<!--     Login button-->
    <Button fx:id="Login"
            text="Login"
            style="-fx-font-size: 16px; -fx-padding: 10px 20px;"
            onAction="#onButtonLoginClick"/>

</VBox>

